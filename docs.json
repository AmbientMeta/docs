        "groups": [
          {
            "group": "API Reference",
            "pages": [
              "api-reference/sanitize",
              "api-reference/rehydrate",
              "api-reference/feedback",
              "api-reference/patterns",
              "api-reference/insights",
              "api-reference/health",
              "api-reference/errors"
            ]
          }
        ]
      },
      {
        "tab": "SDKs",
        "groups": [
          {
            "group": "SDKs",
            "pages": [
              "sdks/python"
            ]
          }
