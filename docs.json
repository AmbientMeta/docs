{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "AmbientMeta",
  "logo": {
    "dark": "/logo/dark.png",
    "light": "/logo/light.png",
    "href": "https://ambientmeta.com"
  },
  "favicon": "/Favicon64x64.png",
  "colors": {
    "primary": "#4a90d9",
    "light": "#5ba0e9",
    "dark": "#1a1a2e"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://ambientmeta.com/dashboard"
      }
    ],
    "primary": {
      "type": "button",
